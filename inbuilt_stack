#include<iostream>
#include<stack>  // include stack  i.e inbuilt stack code 
using namespace std;

int main(){
stack<int>s ;
s.push(10) ;
s.push(20) ;
s.push(30) ;
cout << " top element is :" << s.top() << "\n" ;
s.pop() ;
cout << " top element is :" << s.top() << "\n" ;
cout << "is stack empty:" << s.empty() << "\n" ;
cout << "size of stack :" << s.size() << "\n" ;
}
